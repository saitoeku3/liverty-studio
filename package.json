  "private": true,
  "scripts": {
    "build": "rimraf ./dist && yarn build:main && yarn build:renderer",
    "build:main": "rimraf ./dist/main.js && webpack --config ./webpack/webpack.config.main.ts",
    "build:renderer": "webpack --config ./webpack/webpack.config.renderer.ts",
    "dev": "concurrently \"yarn dev:renderer\" \"yarn dev:main\"",
    "dev:main": "yarn build:main && electron ./dist/main.js",
    "dev:renderer": "webpack-dev-server --port 3000 --config ./webpack/webpack.config.renderer.ts",
    "format": "yarn lint --fix",
    "lint": "eslint \"**/*.{ts,tsx}\""
  },
