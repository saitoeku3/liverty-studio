{
  "name": "liverty-studio",
  "version": "0.0.1",
  "private": true,
  "author": "Tadao Iseki <saitoeku3@gmail.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/saitoeku3/liverty-stadio.git"
  },
  "license": "MIT",
  "scripts": {
    "build": "rimraf ./dist && npm run build:main && npm run build:renderer",
    "build:main": "rimraf ./dist/main.js && webpack --config ./webpack/webpack.config.main.ts",
    "build:renderer": "webpack --config ./webpack/webpack.config.renderer.ts",
    "dev": "concurrently \"npm run dev:renderer\" \"npm run dev:main\"",
    "dev:main": "npm run build:main && electron ./dist/main.js",
    "dev:renderer": "webpack-dev-server --port 8080 --config ./webpack/webpack.config.renderer.ts --hot",
    "format": "eslint \"src/**/*.{ts,tsx}\" --fix",
    "lint": "eslint \"src/**/*.{ts,tsx}\"",
    "test": "jest --config jest.config.json"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.25",
    "@fortawesome/free-solid-svg-icons": "^5.11.2",
    "@fortawesome/react-fontawesome": "^0.1.8",
    "@hot-loader/react-dom": "^16.11.0",
    "@pixiv/three-vrm": "^0.2.0",
    "facefilter": "^1.1.4",
    "image-size": "^0.8.3",
    "konva": "^4.0.16",
    "react": "16.10.2",
    "react-dom": "16.10.2",
    "react-hot-loader": "^4.12.16",
    "react-konva": "^16.10.1-0",
    "react-redux": "^7.1.3",
    "react-router-dom": "^5.1.2",
    "react-three-fiber": "3.0.3",
    "redux": "^4.0.4",
    "redux-thunk": "^2.3.0",
    "server-destroy": "^1.0.1",
    "styled-components": "^4.4.1",
    "three": "0.108.0",
    "use-image": "^1.0.5",
    "uuid": "^3.3.3",
    "ws": "^7.2.1"
  },
  "devDependencies": {
    "@types/dom-mediacapture-record": "^1.0.2",
    "@types/electron-load-devtool": "^1.0.1",
    "@types/html-webpack-plugin": "^3.2.1",
    "@types/jest": "^24.0.24",
    "@types/node": "^13.1.0",
    "@types/react": "^16.9.17",
    "@types/react-dom": "^16.9.4",
    "@types/react-redux": "^7.1.5",
    "@types/react-router-dom": "^5.1.3",
    "@types/server-destroy": "^1.0.0",
    "@types/styled-components": "^4.4.1",
    "@types/uuid": "^3.4.6",
    "@types/webpack": "^4.39.8",
    "@types/webpack-merge": "^4.1.5",
    "@types/ws": "^6.0.4",
    "@typescript-eslint/eslint-plugin": "^2.13.0",
    "@typescript-eslint/parser": "^2.12.0",
    "concurrently": "^5.0.1",
    "electron": "^7.1.5",
    "electron-load-devtool": "^1.2.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.5.0",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.17.0",
    "eslint-plugin-react-hooks": "^2.2.0",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^3.0.9",
    "jest": "^24.9.0",
    "lint-staged": "^9.5.0",
    "prettier": "^1.19.0",
    "redux-devtools": "^3.5.0",
    "rimraf": "^3.0.0",
    "ts-jest": "^24.2.0",
    "ts-loader": "^6.2.1",
    "ts-node": "^8.5.4",
    "typescript": "^3.7.4",
    "webpack": "^4.41.3",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.10.1",
    "webpack-merge": "^4.2.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "npm run format",
      "git add"
    ]
  }
}
